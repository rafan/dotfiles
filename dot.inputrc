#"\x7F": backward-delete-char
"\e[3~": delete-char
"\e[1~": beginning-of-line
"\e[4~": end-of-line
# CJK
set convert-meta off
set input-meta on
set output-meta on
set convert-meta off
#
set editing-mode vi

"\e[5~": history-search-backward
"\e[6~": history-search-forward
